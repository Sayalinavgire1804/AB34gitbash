package Practice;

public class Array4 {

	public static void main(String[] args) {

		String[] City=new String[3];
		City[0]="Mumbai";
		City[1]="Pune";
		City[2]="Nagpur";
		City[3]="Delhi";
		String[] Visitor=new String[3];
		Visitor[0]="True";
		Visitor[1]="True";
		Visitor[2]="False";
		Visitor[3]="False";
		
	String[] Message=new String[3];
	Message[0]="You have been there";
	Message[1]="You have been there";
	Message[2]="You will visit soon";
	Message[3]="You will visit soon";
	for(int i=0;i<3;i++)
	{
		System.out.print(City[i].concat(" ")+Visitor[i].concat(" ")+Message[i]);
		System.out.println();
		
	}
		
				
	}

}
