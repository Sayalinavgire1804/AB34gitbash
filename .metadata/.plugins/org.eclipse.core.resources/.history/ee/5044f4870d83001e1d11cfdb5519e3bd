public class grouping_class {
	@Test(groups= {"Smoke"})
	public void scenario1()
	{
		
	}
	@Test(groups={"Smoke"})
	public void scenario2()
	{
		
	}
	@Test(groups= {"Smoke","System"})
	public void scenario3()
	{
		
	}
	@Test(groups= {"Smoke","System","Regression"})
	public void scenario4()
	{
		
	}
	@Test
	public void scenario5()
	{
		
	}
	

}
