package Throws;

public class Ages {
	public static void main(String[] args) throws ArithmeticException
	{
		int a=15;
		if(a>=18)
		{
			System.out.println("Welcome to google");

			
		} else
			

		{
			try {
				Thread.sleep(3000);
			} catch (InterruptedException e) {
				System.out.println("Handling InterruptedException Exception");
				e.printStackTrace();
			}
			throw new ArithmeticException("You are getting exception");
		}
		
	}

}
